{
  "manifest_version": 2,
  "name": "Sakharov Report Table Alignment",
  "description": "Align tables for Sakharov report",
  "version": "0.0.2",
  "permissions": [
    "activeTab",
    "http://www2s.biglobe.ne.jp/~sakharov/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://www2s.biglobe.ne.jp/~sakharov/*"],
      "js": ["myscript.js"],
      "run_at": "document_end"
    }
  ]
}
